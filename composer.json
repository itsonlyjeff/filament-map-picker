{
    "name": "itsonlyjeff/filament-map-picker",
    "description": "This is my package filament-map-picker",
    "keywords": [
        "itsonlyjeff",
        "laravel",
        "filament-map-picker"
    ],
    "homepage": "https://github.com/itsonlyjeff/filament-map-picker",
    "support": {
        "issues": "https://github.com/itsonlyjeff/filament-map-picker/issues",
        "source": "https://github.com/itsonlyjeff/filament-map-picker"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "itsonlyjeff",
            "email": "jeffrey92.hrb@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "filament/filament": "^4.0",
        "spatie/laravel-package-tools": "^1.19.0",
        "illuminate/contracts": "*"
    },
    "autoload": {
        "psr-4": {
            "ItsOnlyJeff\\FilamentMapPicker\\": "src/",
            "ItsOnlyJeff\\FilamentMapPicker\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ItsOnlyJeff\\FilamentMapPicker\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ItsOnlyJeff\\FilamentMapPicker\\FilamentMapPickerServiceProvider"
            ],
            "aliases": {
                "FilamentMapPicker": "ItsOnlyJeff\\FilamentMapPicker\\Facades\\FilamentMapPicker"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "nunomaduro/collision": "*",
        "laravel/pint": "*",
        "orchestra/testbench": "*",
        "pestphp/pest": "*",
        "pestphp/pest-plugin-arch": "*",
        "pestphp/pest-plugin-laravel": "*",
        "phpstan/extension-installer": "*",
        "phpstan/phpstan-deprecation-rules": "*",
        "phpstan/phpstan-phpunit": "*"
    }
}
